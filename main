def StartBtn_pressed():
    global points
    global time
    points = 0
    time = 300
    Score.config(text=points)
    #Timer1.config(text=time)
    Timer1.config(text=str(time))
    print("Button pressed is start")

def ResetBtn_pressed():
    global points
    global time
    points = 0 
    time = 0
    Score.config(text=points)
    #Timer1.config(text=time)
    Timer1.config(text=str(time))
    print("Button pressed is reset")

def StartBtnTimer():
    StartBtn_pressed()
    Timer()

def Check0():
    global ans
    global points
    if ans == 0:
