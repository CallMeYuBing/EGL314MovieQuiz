def Timer():
    global time
    time = time-1
    if time <= 0 : 
        return
    Timer1.config(text=str(time))
    Timer1.after(1000,Timer)

#Check if the condition to win or lose the game is met
def GameCondition():
    global time
    global points

    if time == 0 and points < 8: 
        messagebox.showinfo("", "You lose!" )
        ResetBtn_pressed()
    
    elif points >= 8 and time == 0 : 
        messagebox.showinfo("Win","You win!") 
        ResetBtn_pressed()
    
    elif points == 12: 
        messagebox.showinfo("Win","You got everything correct!" )
        ResetBtn_pressed()
    
    
    main.after(1000,GameCondition)


def RulesBtn():
    messagebox.showinfo("Rules","Guess the movie through the emoji displayed on the panel! /n The goal is answer all 12 questions correctly and score 12 points before the 5 minute time limit is up!")


